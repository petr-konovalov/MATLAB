function res = getDescreteSol(N)
    h = 1 / N;
    x = 0: h: 1;
    gPhi = phi(x);
    C = zeros(1, N - 1);
    res = zeros(6, 6);
    
    for p = 1: N - 1
        C(p) = h * gPhi * Xi(p, x)';
    end
    
    for j = 1: 6
        xi = (j - 1) * 0.2;
        for k = 1: 6
            tk = (k - 1) * 0.02;
            for p = 1: N - 1
                res(j, k) = res(j, k) + C(p) * T(p, tk) * sin(p * pi
            end
            res(j, k) = sqrt(2) * res(j, k);
        end
    end
end